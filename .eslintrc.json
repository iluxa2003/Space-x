{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-props-no-spreading": "off",
    "react/require-default-props": ["error", { "functions": "defaultArguments" }]
  }
}
