{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "prettier",
        "airbnb" 
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "only-warn"
    ],
    "rules": {
        "quotes":"off",
        "linebreak-style":"off",
        "react/jsx-props-no-spreading":"off",
        "react/forbid-prop-types":"off"
    }
}